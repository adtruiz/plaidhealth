{
  "name": "@plaidhealth/api",
  "version": "1.0.0",
  "description": "FHIR integration API for Plaid for Healthcare",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "npx jest --coverage --forceExit",
    "test:watch": "npx jest --watch --forceExit",
    "migrate": "node src/migrations/run-migrations.js",
    "db:setup": "node src/db-setup.js"
  },
  "keywords": [
    "fhir",
    "healthcare",
    "epic",
    "ehr",
    "patient"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.0",
    "compression": "^1.8.1",
    "connect-pg-simple": "^9.0.1",
    "connect-redis": "^7.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.11.3",
    "redis": "^4.6.13",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "jest": "^30.2.0",
    "nodemon": "^3.0.1",
    "supertest": "^7.1.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/server.js"
    ],
    "testMatch": [
      "**/tests/**/*.test.js"
    ],
    "setupFilesAfterEnv": [
      "./tests/setup.js"
    ]
  }
}
