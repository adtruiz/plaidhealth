{
  "name": "plaidhealth",
  "version": "1.0.0",
  "description": "Plaid for Healthcare - Unified API for patient health data aggregation",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:api": "npm run dev --workspace=@plaidhealth/api",
    "dev:portal": "npm run dev --workspace=@plaidhealth/developer-portal",
    "dev:marketing": "npm run dev --workspace=@plaidhealth/marketing-site",
    "build": "npm run build --workspaces --if-present",
    "test": "npm run test --workspaces --if-present"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adtruiz/plaidhealth.git"
  },
